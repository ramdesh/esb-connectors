<root>
	<apiUrl>%s(amazonSimpleDBApiUrl)</apiUrl>
	<accessKeyId>%s(accessKeyId)</accessKeyId>
	<secretAccessKey>%s(secretAccessKey)</secretAccessKey>
	<action>Select</action>
	<version>%s(version)</version>
	<signatureVersion>%s(signatureVersion)</signatureVersion>
	<signatureMethod>%s(signatureMethod)</signatureMethod>
	<selectExpression>select * from %s(domainName)</selectExpression>
	<consistentRead>true</consistentRead>
</root>